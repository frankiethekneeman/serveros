{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
        "includePattern": ".+\\.js(doc)?$",
        "include": [
            "README.md",
            "src",
            "index.js"
        ],
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [],
    "opts": {
        "encoding": "utf8",
        "destination": "./jsdoc/output",
        "recurse": "true",
        "readme": "./README.md",
        "private": "true"
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": false
    }
}
